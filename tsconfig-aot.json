{
	"compilerOptions": {
		"target": "es5",
		"module": "es2015",

		"moduleResolution": "node",

		"declaration": false,
		"noEmitHelpers": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,

		"lib": [ "es5", "es2015", "dom" ],

		"noImplicitAny": true,
		"suppressImplicitAnyIndexErrors": true,

		"allowUnreachableCode": false,
		"noUnusedParameters": false,
		"noUnusedLocals": false

	}
}

